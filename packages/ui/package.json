    "@turbo/gen": "^2.4.0",
    "@types/node": "^22",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "eslint": "^9.20.0",
    "tsup": "^8.0.2",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}